        "# 侍テラコヤ提出課題\n",
        "# Pythonの基礎を学ぼう -> 繰り返し処理を使いこなそう\n",
        "\n",
        "# リストの中身を先頭から全て取り出すプログラム\n",
        "\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "i = 0\n",
        "\n",
        "# for分で取り出す\n",
        "for planet in array:\n",
        "  print(planet)\n",
        "\n",
        "# while分で取り出す\n",
        "while i < len(array):\n",
        "  print(array[i])\n",
        "  i += 1"
      ]
