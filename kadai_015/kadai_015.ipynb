      "source": [
        "# 侍テラコヤ提出課題\n",
        "# Pythonの基礎を学ぼう -> クラスを使いこなそう\n",
        "\n",
        "# 名前と年齢を出力するプログラム\n",
        "\n",
        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"名前：{self.name}\")\n",
        "    print(f\"年齢：{self.age}\")\n",
        "\n",
        "tanaka = Human(\"田中太郎\", \"28\")\n",
        "tanaka.printinfo()"
      ]
    }
  ]
